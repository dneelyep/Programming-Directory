// Declaring, manipulating, using variables

#include <iostream>
using namespace std;

int main ()
{
  // Declaring all mah variables here
  int a;
  int b;
  int result;

  // Instructions - self-explanatory
  a = 5;
  b = 2;
  a = a + 1;
  result = a - b;

  // And the result:
  cout << result;

  // KILLEET! NAO!
  return 0;
}
